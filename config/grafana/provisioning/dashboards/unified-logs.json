            { "field": "@timestamp", "id": "dtH", "settings": { "interval": "auto" }, "type": "date_histogram" }
          ],
          "metrics": [{ "id": "mH", "type": "count" }],
          "query": "message:\"\\\"log.level\\\":\\\"panic\\\"\"",
          "refId": "H",
          "timeField": "@timestamp"
