            { "field": "@timestamp", "id": "dtD", "settings": { "interval": "auto" }, "type": "date_histogram" }
          ],
          "metrics": [{ "id": "mD", "type": "count" }],
          "query": "message:\"\\\"log.level\\\":\\\"info\\\"\"",
          "refId": "D",
          "timeField": "@timestamp"
        },
        {
          "alias": "WARN",
          "bucketAggs": [
            { "field": "@timestamp", "id": "dtE", "settings": { "interval": "auto" }, "type": "date_histogram" }
          ],
          "metrics": [{ "id": "mE", "type": "count" }],
          "query": "message:\"\\\"log.level\\\":\\\"warn\\\"\"",
          "refId": "E",
          "timeField": "@timestamp"
        },
        {
          "alias": "ERROR",
          "bucketAggs": [
            { "field": "@timestamp", "id": "dtF", "settings": { "interval": "auto" }, "type": "date_histogram" }
          ],
          "metrics": [{ "id": "mF", "type": "count" }],
          "query": "message:\"\\\"log.level\\\":\\\"error\\\"\"",
